<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="batteryproject.cn.batteryproject">

    <uses-permission android:name="android.permission.BATTERY_STATS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/c"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dummy.MainActivity" />
        <activity android:name=".dummy.AboutActivity" />
        <activity android:name=".dummy.FragmentActivity" />
        <activity android:name=".dummy.TransitionActivity">
        </activity>
        <activity android:name=".dummy.adActivity" />
        <activity android:name=".dummy.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>